, CART_REMOVE_ITEM, CART_CHANGE_ITEM } from \"../types\";\nimport axios from \"axios\";\n\nexport const addToCard = (id) => async (dispatch, getState) => {\n  try {\n    const { data } = await axios.get(`/api/products/${id}`);\n    dispatch({ type: CART_ADD_ITEM, payload: data });\n    localStorage.setItem(\n      \"cartList\",\n      JSON.stringify(getState().cartList.cartList)\n    );\n  } catch {}\n};\n","/Users/arahis/Projects/e-mern/client/src/redux/reducers/userReducer.js",[],"/Users/arahis/Projects/e-mern/client/src/pages/login/index.js",["122"],"import React, { useState, useEffect } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { Form, Button, Row, Col, Spinner, Container } from \"react-bootstrap\";\nimport FormContainer from \"../../components/FormContainer\";\nimport { Link } from \"react-router-dom\";\nimport { login } from \"../../redux/actions/userActions\";\n\nconst Login = ({ location, history }) => {\n  const [email, setEmail] = useState(\"\");\n  const [password, setPassword] = useState(\"\");\n\n  const dispatch = useDispatch();\n  const userLogin = useSelector((state) => state.userLogin);\n  const { loading, error, userData } = userLogin;\n\n  const redirect = location.search ? location.search.split(\"=\")[1] : \"/\";\n  useEffect(() => {\n    if (userData) {\n      history.push(redirect);\n    }\n  }, [history, userData, redirect]);\n  const submitHandler = (e) => {\n    e.preventDefault();\n    dispatch(login(email, password));\n  };\n\n  return (\n    <FormContainer>\n      <h1>Sign in</h1>\n      {error && <div>{error}</div>}\n      {loading && <Spinner />}\n      <Form onSubmit={submitHandler}>\n        <Form.Group controlId=\"email\">\n          <Form.Label>Email address</Form.Label>\n          <Form.Control\n            type=\"email\"\n            placeholder=\"Enter email\"\n            value={email}\n            onChange={(e) => setEmail(e.target.value)}\n          />\n        </Form.Group>\n        <Form.Group controlId=\"pssword\">\n          <Form.Label>Password</Form.Label>\n          <Form.Control\n            type=\"password\"\n            placeholder=\"Password\"\n            value={password}\n            onChange={(e) => setPassword(e.target.value)}\n          />\n        </Form.Group>\n        <Button variant=\"primary\" type=\"submit\">\n          Sign In\n        </Button>\n      </Form>\n      <Row>\n        <Col>\n          New Customer?\n          <Link\n            to={redirect ? `/register/redirect=${redirect}` : `/register`}\n            className=\"ml-1\"\n          >\n            Register\n          </Link>\n        </Col>\n      </Row>\n    </FormContainer>\n  );\n};\n\nexport default Login;\n","/Users/arahis/Projects/e-mern/client/src/redux/actions/userActions.js",[],"/Users/arahis/Projects/e-mern/client/src/pages/register/index.js",["123"],"/Users/arahis/Projects/e-mern/client/src/components/FormContainer.js",[],{"ruleId":"124","replacedBy":"125"},{"ruleId":"126","replacedBy":"127"},{"ruleId":"128","severity":1,"message":"129","line":10,"column":30,"nodeType":"130","messageId":"131","endLine":10,"endColumn":35},{"ruleId":"124","replacedBy":"132"},{"ruleId":"126","replacedBy":"133"},{"ruleId":"128","severity":1,"message":"134","line":8,"column":3,"nodeType":"130","messageId":"131","endLine":8,"endColumn":10},{"ruleId":"128","severity":1,"message":"135","line":2,"column":10,"nodeType":"130","messageId":"131","endLine":2,"endColumn":14},{"ruleId":"128","severity":1,"message":"136","line":3,"column":23,"nodeType":"130","messageId":"131","endLine":3,"endColumn":33},{"ruleId":"128","severity":1,"message":"137","line":5,"column":3,"nodeType":"130","messageId":"131","endLine":5,"endColumn":6},{"ruleId":"128","severity":1,"message":"138","line":6,"column":3,"nodeType":"130","messageId":"131","endLine":6,"endColumn":6},{"ruleId":"128","severity":1,"message":"139","line":7,"column":3,"nodeType":"130","messageId":"131","endLine":7,"endColumn":8},{"ruleI